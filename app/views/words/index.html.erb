<h1>Wordzies!</h1>

<%= form_tag("/search", :method => "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<%= @definitions.first["word"]%>

<ul>
<% @definitions.each do |definition| %>
<li><%= definition["text"] %></li>
<% end %>
</ul>