<!DOCTYPE html>
<html>
<head>
  <title>WordziesCA</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<%= form_tag({:controller => 'words', :action => 'search'}, :method => "get") do %>
	  <%= label_tag(:q, "Search for:") %>
	  <%= text_field_tag(:q) %>
	  <%= submit_tag("Search") %>
	<% end %>
	
<%= yield %>

</body>
</html>
